cc,e) => acc += `<option value=\"${e.id}\">${e.name}</option>`, ``),\n\t\"3rd\": spellGroups[\"3rd\"].reduce((acc,e) => acc += `<option value=\"${e.id}\">${e.name}</option>`, ``),\n\t\"4th\": spellGroups[\"4th\"].reduce((acc,e) => acc += `<option value=\"${e.id}\">${e.name}</option>`, ``),\n\t\"5th\": spellGroups[\"5th\"].reduce((acc,e) => acc += `<option value=\"${e.id}\">${e.name}</option>`, ``)\n};\n\n// define the sub-level selections:\nconst subSelections = {};\nfor(let i = 0; i < maxCombinedSpellLevel; i++){\n\tsubSelections[nth(i+1)] = `<select id=\"sub${i+1}\" name=\"${i+1}\"><option value=\"\">&mdash;</option></select>`;\n}\n\n// define the top selections' options:\nconst options = Array.fromRange(maxCombinedSpellLevel).reduce((acc, e) => acc += `<option value=\"${e+1}\" style=\"font-family: 'Courier New', monospace;\">${nth(e+1)}-level spell</option>`, `<option value=\"0\">&mdash;</option>`);\n\n// define the top selections:\nconst topSelects = Array.fromRange(maxCombinedSpellLevel).reduce((acc, e) => acc += `\n<hr>\n<form>\n\t<div class=\"form-group\">\n\t\t<label for=\"top${e+1}\">Spell level:</label>\n\t\t<select id=\"top${e+1}\" name=\"${e+1}\">${options}</select>\n\t</div>\n\t<div class=\"form-group\">\n\t\t${subSelections[nth(e+1)]}\n\t</div>\n</form>\n`, ``);\n\n// define text that keeps track of combined level:\nconst topText = `<p class=\"levelTrack\">...</p>`;\n\n\nconst dialog = new Dialog({\n\tcontent: topText + topSelects + \"<hr>\",\n\ttitle: \"...\",\n\tbuttons: {\n\t\tgo: {\n\t\t\ticon: `<i class=\"fas fa-check\"></i>`,\n\t\t\tlabel: \"Create Scrolls\",\n\t\t\tcallback: async (html) => {\n\t\t\t\tconst spellScrolls = [];\n\t\t\t\tfor(let i = 0; i < maxCombinedSpellLevel; i++){\n\t\t\t\t\tlet id = html[0].querySelector(`select[id=sub${i+1}]`)?.value;\n\t\t\t\t\tif(!id) continue;\n\t\t\t\t\t\n\t\t\t\t\tconst spell = actor.items.get(id);\n\t\t\t\t\tif(!spell) continue;\n\t\t\t\t\t\n\t\t\t\t\tlet spellScroll = await Item.implementation.createScrollFromSpell(spell);\n\t\t\t\t\tspellScroll = spellScroll.toObject();\n\t\t\t\t\tspellScroll[\"flags.world.murkBoon\"] = true;\n\t\t\t\t\tif(spell.getChatData()?.components?.concentration) spellScroll[\"flags.itemacro\"] = itemMacroObj;\n\t\t\t\t\tspellScroll.name = spellScroll.name.replace(\"Spell Scroll:\", \"Murk Scroll:\");\n\t\t\t\t\tspellScrolls.push(spellScroll);\n\t\t\t\t}\n\t\t\t\tawait actor.createEmbeddedDocuments(\"Item\", spellScrolls);\n\t\t\t\treturn ui.notifications.info(`Created ${spellScrolls.length} scrolls.`);\n\t\t\t}\n\t\t}\n\t},\n\trender: async (html) => {\n\t\thtml[0].querySelector(\"p[class=levelTrack]\").innerHTML = getTopText(html);\n\t\tfor(let i = 0; i < maxCombinedSpellLevel; i++){\n\t\t\thtml[0].querySelector(`select[id=top${i+1}]`)?.addEventListener(\"change\", function(){\n\t\t\t\t// get current value of this topselection:\n\t\t\t\tlet value = html[0].querySelector(`select[id=top${i+1}]`)?.value;\n\t\t\t\tvalue = nth(Number(value));\n\t\t\t\t\n\t\t\t\t// set accum in top text:\n\t\t\t\thtml[0].querySelector(\"p[class=levelTrack]\").innerHTML = getTopText(html);\n\t\t\t\t\n\t\t\t\t// set subselection options:\n\t\t\t\thtml[0].querySelector(`select[id=sub${i+1}]`).innerHTML = spellOptions[value];\n\t\t\t});\n\t\t}\n\t}\n});\n\ndialog.render(true);\n\n\n\nfunction getTopText(html){\n\tlet accum = 0;\n\tfor(let i = 0; i < maxCombinedSpellLevel; i++){\n\t\tlet value = html[0].querySelector(`select[id=top${i+1}]`).value;\n\t\taccum += Number(value);\n\t}\n\treturn `Combined spell level of selections: ${accum} / ${maxCombinedSpellLevel}`;\n}\n\n\n\nfunction nth(n){return n + ([\"st\",\"nd\",\"rd\"][((n+90)%100-10)%10-1]||\"th\")}\nfunction spellSort(a,b){return a.name < b.name ? -1 : a.name > b.name ? 1 : 0}\n\nconst itemMacroObj = {\n    \"macro.data\": {\n\t\t_id: null,\n\t\tname: \"Murk Scroll: Dispel Magic\",\n\t\ttype: \"script\",\n\t\tauthor: \"FRHavFG1029uVyVE\",\n\t\timg: \"icons/svg/dice-target.svg\",\n\t\tscope: \"global\",\n\t\tcommand: \"const roll = await item.roll();\\nif(!roll) return;\\nConcentrationNotifier.applyConcentrationOnItem(item);\",\n\t\tfolder: null,\n\t\tsort: 0,\n\t\t\"permission.default\": 0,\n\t\tflags: {}\n\t}\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"core":{"sourceId":"Item.1h5EBuI7wuBkBauH"},"custom-character-sheet-sections":{"sectionName":""}}},{"_id":"dqZn1GcNQICm1Qmn","name":"Ordus Initiate","type":"background","img":"icons/environment/settlement/bridge-stone.webp","data":{"description":{"value":"<p>You gain several traits depending on your history, your place of origin, and the deity that you favor most.</p>\n<hr /><details>\n<summary>Origin Traits</summary>\n<p>You gain the following proficiencies that should fit your character.</p>\n<p><em><strong>Skills.</strong></em> You gain proficiency in two skills of your choice.</p>\n<ul>\n<li>Survival, Perception</li>\n</ul>\n<p><em><strong>Language.</strong></em> You can speak, read, and write Common.</p>\n<p><em><strong>Proficiencies.</strong></em> You learn two additional languages, or you gain proficiency with two tools, or one of each.</p>\n<ul>\n<li>Draconic, Infernal</li>\n</ul>\n<p><em><strong>Origin.</strong></em> Related to your origin, who you were before you became an adventurer, you gain three minor features; these usually consist one active feature, one passive feature, and one proficiency.</p>\n<ul>\n<li>You can cast the <em>find familiar</em> spell, but only as a ritual, to summon Alyk the tressym.</li>\n<li>You have a +5 bonus to your passive Investigation and Perception.</li>\n<li>You have proficiency in Investigation.</li>\n</ul>\n<hr /></details><details>\n<summary>Piety Traits</summary>\n<p>You gain the following two traits depending on your deity.</p>\n<p><em><strong>Piety.</strong></em> You start with a Piety score of 1. This score increases or decreases over time depending on your actions. Your Piety score is directly related to your favored deity; if you renounce this deity and decide to follow a different one, you lose this ability score and gain a Piety score of 1 with your new deity instead.</p>\n<p><em><strong>Boon.</strong></em> You gain one @Compendium[zhell-custom-stuff.catalog-of-journals.SO6RVIisP2BGHLCg]{minor boon} related to your favored deity.</p>\n</details>","chat":"","unidentified":""},"source":"","advancement":[],"attunement":null},"effects":[],"folder":null,"sort":0,"permission":{"default":2,"ssK6qqhMHUvM1A06":3},"flags":{"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[]}]},"core":{"sourceId":"Item.033HfO5S9gkxuywJ"}}},{"_id":"oA7bBuR4OgdNKeSz","name":"Weave","type":"feat","img":"https://assets.forge-vtt.com/6031826c83ef59f0ff7fedfa/modules/zhell-custom-stuff/images/symbols/holy_murk.webp","data":{"description":{"value":"<p>You can use any spell scroll you find regardless of whether or not the spell is on your spell list, but only if the spell on the scroll has a spell level lower than or equal to half your proficiency bonus.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":null,"actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"Murk 1","recharge":{"value":null,"charged":false},"attunement":0},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"ssK6qqhMHUvM1A06":3},"flags":{"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[]}]},"custom-character-sheet-sections":{"sectionName":""},"core":{"sourceId":"Item.0em0DF6YMerPxOAM"},"dnd5e":{"vehicleCargo":false}}},{"_id":"yKDhGn7IezfBiSuI","name":"Human","type":"feat","img":"icons/environment/people/commoner.webp","data":{"description":{"value":"<details>\n<summary>Human</summary>\n<p>A significant quantity of the Havilonian settlers were humans, and they make up almost half of the population to this day. Varied in skin tone and mannerisms, the settling humans have become a melting pot of different reflections and dialects.</p>\n<p>The humans of Havilon are experts at adapting to their environment. For example, the humans of the Eastern Sunlands are accustomed to the blazing heat and are visually distinct from their cousins, most prominently by an always healthy umber reflection, and live only slightly shorter lives than other humans and are often slimmer.</p>\n</details><details open=\"open\">\n<summary>Human Traits</summary>\n<p><em>Ability Score Increases: +1 Wisdom, +1 Intelligence, +1 Dexterity</em></p>\n<p>As a human, you have the following traits.</p>\n<hr />\n<p><em><strong>Age.</strong></em> Humans reach adulthood in their late teens and live less than a century.</p>\n<p><em><strong>Size.</strong></em> Humans vary widely in height and build, from barely 5 feet to well over 6 feet tall. Regardless of your position in that range, your size is Medium.</p>\n<p><em><strong>Speed.</strong></em> Your base walking speed is 30 feet.</p>\n<p><em><strong>Creature Type.</strong></em> You are Humanoid.</p>\n<hr />\n<p><em>1st-level human traits</em></p>\n<p>@Compendium[zhell-custom-stuff.catalog-of-races.DwHdBNl4HhXgGmw5]{Expert}</p>\n<p>@Compendium[zhell-custom-stuff.catalog-of-races.E2AvCU1IU8R6tuB7]{Improvisation}</p>\n<p><em><strong>Regional Adaptation.</strong></em> Most humans of Havilon have adapted to their native region. You pick one of these options depending where you hail from.</p>\n<ul>\n<li>@Compendium[zhell-custom-stuff.catalog-of-races.UZEJauCo1E3qphwW]{Extreme Heat}</li>\n<li><span style=\"text-decoration:line-through\">@Compendium[zhell-custom-stuff.catalog-of-races.yYgu0kHIhzVyP5H8]{Extreme Cold}</span></li>\n<li><span style=\"text-decoration:line-through\">@Compendium[zhell-custom-stuff.catalog-of-races.4bM1i8xxjnN7WtEk]{Heavy Precipitation}</span></li>\n<li><span style=\"text-decoration:line-through\">@Compendium[zhell-custom-stuff.catalog-of-races.IHnlwHwxE1w1tSk4]{High Altitudes}</span></li>\n<li><span style=\"text-decoration:line-through\">@Compendium[zhell-custom-stuff.catalog-of-races.b6K6DtQeDNXsOxNo]{Wilderness}</span></li>\n</ul>\n<hr />\n<p><em>6th-level human traits</em></p>\n<p>@Compendium[zhell-custom-stuff.catalog-of-races.sM39rmYxl6IlsR8m]{Growing Improvement}</p>\n</details>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"","recharge":{"value":null,"charged":false},"attunement":null},"effects":[],"folder":null,"sort":200000,"permission":{"default":0,"ssK6qqhMHUvM1A06":3},"flags":{"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[]}]},"custom-character-sheet-sections":{"sectionName":"Racial Traits"},"core":{"sourceId":"Item.LheCW9MBAEEW1mli"}}},{"_id":"3BigrlbLBZChO2Cm","name":"Expert","type":"feat","img":"icons/skills/trades/academics-merchant-scribe.webp","data":{"description":{"value":"<p>Pick one skill or tool in which you are proficient. Your proficiency bonus is doubled when you make an ability check that uses that skill or tool. The chosen skill or tool can be one you gained from any source.</p>\n<p><em>(Investigation)</em></p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"widt