ons.fire.cone.blue.02',\n\t'jb2a.breath_weapons.fire.cone.purple.02']);\n\nconst rayLengthFt = 30 + 2.5;\nconst rayLengthPx = canvas.grid.size * (rayLengthFt + 2.5) / canvas.scene.data.gridDistance;\nconst startPoint = {x: token.center.x, y: token.center.y};\nlet angle = 0;\nlet start = 0;\nlet request;\nconst fill = game.user.data.color;\nconst pixelFeetRatio = canvas.grid.size / canvas.scene.data.gridDistance;\n\nlet mpx, mpy;\n\nasync function onLoad(){\n    let cone = new PIXI.Graphics();\n    cone.beginFill(game.user.color.replace(\"#\", \"0x\"));\n    cone.drawPolygon(\n        startPoint.x, startPoint.y,\n        startPoint.x + 0.5 * rayLengthPx * pixelFeetRatio, startPoint.y + rayLengthPx * pixelFeetRatio,\n        startPoint.x - 0.5 * rayLengthPx * pixelFeetRatio, startPoint.y + rayLengthPx * pixelFeetRatio\n        );\n    cone.endFill();\n    cone.name = \"tempCone\";\n    cone.alpha = 0.3;\n    canvas.app.stage.addChild(cone);\n    requestAnimationFrame(step);\n}\nfunction step(timestamp) {\n    let marker = canvas.app.stage.children.find(c => c.name == \"tempCone\");\n    if (start === 0) start = timestamp;\n    const elapsed = timestamp - start;\n    const mousePos = canvas.app.renderer.plugins.interaction.mouse.getLocalPosition(canvas.app.stage);\n    angle = mousePos.x === startPoint.x ? toDegrees(Math.PI / 2) : \n        mousePos.x > startPoint.x ? toDegrees(Math.PI - Math.atan((startPoint.y - mousePos.y)/(mousePos.x - startPoint.x))) :\n        mousePos.x < startPoint.x ? toDegrees(Math.atan((mousePos.y - startPoint.y)/(mousePos.x - startPoint.x))) : 0; //Math.ceil(elapsed * 360 / 1000);\n    canvas.app.stage.removeChild(canvas.app.stage.children.find(c => c.name === \"tempCone\"));\n    let cone = new PIXI.Graphics();\n    cone.beginFill(game.user.color.replace(\"#\", \"0x\"));\n    //lets do some maths\n    const theta = (360 - angle) * Math.PI / 180; \n    const midPointX = startPoint.x + Math.sin(theta) * rayLengthFt * pixelFeetRatio;\n    const midPointY = startPoint.y + Math.cos(theta) * rayLengthFt  * pixelFeetRatio;\n    const rightPointX = midPointX + Math.cos(theta) * rayLengthFt  * pixelFeetRatio / 2;\n    const rightPointY = midPointY - Math.sin(theta) * rayLengthFt  * pixelFeetRatio / 2;\n    const leftPointX = midPointX - Math.cos(theta) * rayLengthFt  * pixelFeetRatio / 2;\n    const leftPointY = midPointY + Math.sin(theta) * rayLengthFt  * pixelFeetRatio / 2;\n\t\n\tmpx = midPointX;\n\tmpy = midPointY;\n    \n    cone.drawPolygon(\n        startPoint.x, startPoint.y,\n        rightPointX, rightPointY,\n        leftPointX, leftPointY\n        );\n    cone.endFill();\n    cone.alpha = 0.3;\n    cone.name = \"tempCone\";\n    canvas.app.stage.addChild(cone);\n    if (elapsed < 1000) request = requestAnimationFrame(step);\n    else { \n        start = 0;\n        request = requestAnimationFrame(step);\n    }\n}\n\nfunction offSet(angle){\n    const halfGrid = canvas.grid.size / 2;\n    const angleR = Math.round(angle/45)*45;\n    let x, y;\n    \n    if([0, 180, 360].includes(angleR)) x = 0;\n    else if([45, 90, 135].includes(angleR)) x = -halfGrid;\n    else x = halfGrid;\n    \n    if([0, 45, 315, 360].includes(angleR)) y = halfGrid;\n    else if([135, 180, 225].includes(angleR)) y = -halfGrid;\n    else y = 0;\n    return [x,y];\n}\n\n\n\nawait PIXI.Loader.shared\n    .load(onLoad);\n\n$(document.body).on(\"click\", async () => {\n    $(document.body).off(\"click\");\n    cancelAnimationFrame(request);\n    canvas.app.stage.removeChild(canvas.app.stage.children.find(c => c.name === \"tempCone\"));\n\tconst x = startPoint.x + offSet(angle)[0];\n\tconst y = startPoint.y + offSet(angle)[1];\n\t\n    \n    let template_data = {\n        angle: 53.13,\n        t: 'cone',\n        user: game.user.id,\n        direction: Math.round(angle + 90),\n        distance: rayLengthFt - 2.5,\n        width: 1,\n        x,\n        y,\n        borderColor: '#000000',\n        fillColor: game.user.color,\n      