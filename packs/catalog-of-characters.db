lue',\n\t'jb2a.breath_weapons.fire.cone.orange.02',\n\t'jb2a.breath_weapons.fire.cone.blue.02',\n\t'jb2a.breath_weapons.fire.cone.purple.02']);\n\nconst rayLengthFt = 30 + 2.5;\nconst rayLengthPx = canvas.grid.size * (rayLengthFt + 2.5) / canvas.scene.data.gridDistance;\nconst startPoint = {x: token.center.x, y: token.center.y};\nlet angle = 0;\nlet start = 0;\nlet request;\nconst fill = game.user.data.color;\nconst pixelFeetRatio = canvas.grid.size / canvas.scene.data.gridDistance;\n\nlet mpx, mpy;\n\nasync function onLoad(){\n    let cone = new PIXI.Graphics();\n    cone.beginFill(game.user.color.replace(\"#\", \"0x\"));\n    cone.drawPolygon(\n        startPoint.x, startPoint.y,\n        startPoint.x + 0.5 * rayLengthPx * pixelFeetRatio, startPoint.y + rayLengthPx * pixelFeetRatio,\n        startPoint.x - 0.5 * rayLengthPx * pixelFeetRatio, startPoint.y + rayLengthPx * pixelFeetRatio\n        );\n    cone.endFill();\n    cone.name = \"tempCone\";\n    cone.alpha = 0.3;\n    canvas.app.stage.addChild(cone);\n    requestAnimationFrame(step);\n}\nfunction step(timestamp) {\n    let marker = canvas.app.stage.children.find(c => c.name == \"tempCone\");\n    if (start === 0) start = timestamp;\n    const elapsed = timestamp - start;\n    const mousePos = canvas.app.renderer.plugins.interaction.mouse.getLocalPosition(canvas.app.stage);\n    angle = mousePos.x === startPoint.x ? toDegrees(Math.PI / 2) : \n        mousePos.x > startPoint.x ? toDegrees(Math.PI - Math.atan((startPoint.y - mousePos.y)/(mousePos.x - startPoint.x))) :\n        mousePos.x < startPoint.x ? toDegrees(Math.atan((mousePos.y - startPoint.y)/(mousePos.x - startPoint.x))) : 0; //Math.ceil(elapsed * 360 / 1000);\n    canvas.app.stage.removeChild(canvas.app.stage.children.find(c => c.name === \"tempCone\"));\n    let cone = new PIXI.Graphics();\n    cone.beginFill(game.user.color.replace(\"#\", \"0x\"));\n    //lets do some maths\n    const theta = (360 - angle) * Math.PI / 180; \n    const midPointX = startPoint.x + Math.sin(theta) * rayLengthFt * pixelFeetRatio;\n    const midPointY = startPoint.y + Math.cos(theta) * rayLengthFt  * pixelFeetRatio;\n    const rightPointX = midPointX + Math.cos(theta) * rayLengthFt  * pixelFeetRatio / 2;\n    const rightPointY = midPointY - Math.sin(theta) * rayLengthFt  * pixelFeetRatio / 2;\n    const leftPointX = midPointX - Math.cos(theta) * rayLengthFt  * pixelFeetRatio / 2;\n    const leftPointY = midPointY + Math.sin(theta) * rayLengthFt  * pixelFeetRatio / 2;\n\t\n\tmpx = midPointX;\n\tmpy = midPointY;\n    \n    cone.drawPolygon(\n        startPoint.x, startPoint.y,\n        rightPointX, rightPointY,\n        leftPointX, leftPointY\n        );\n    cone.endFill();\n    cone.alpha = 0.3;\n    cone.name = \"tempCone\";\n    canvas.app.stage.addChild(cone);\n    if (elapsed < 1000) request = requestAnimationFrame(step);\n    else { \n        start = 0;\n        request = requestAnimationFrame(step);\n    }\n}\n\nfunction offSet(angle){\n    const halfGrid = canvas.grid.size / 2;\n    const angleR = Math.round(angle/45)*45;\n    let x, y;\n    \n    if([0, 180, 360].includes(angleR)) x = 0;\n    else if([45, 90, 135].includes(angleR)) x = -halfGrid;\n    else x = halfGrid;\n    \n    if([0, 45, 315, 360].includes(angleR)) y = halfGrid;\n    else if([135, 180, 225].includes(angleR)) y = -halfGrid;\n    else y = 0;\n    return [x,y];\n}\n\n\n\nawait PIXI.Loader.shared\n    .load(onLoad);\n\n$(document.body).on(\"click\", async () => {\n    $(document.body).off(\"click\");\n    cancelAnimationFrame(request);\n    canvas.app.stage.removeChild(canvas.app.stage.children.find(c => c.name === \"tempCone\"));\n\tconst x = startPoint.x + offSet(angle)[0];\n\tconst y = startPoint.y + offSet(angle)[1];\n\t\n    \n    let template_data = {\n        angle: 53.13,\n        t: 'cone',\n        user: game.user.id,\n        direction: Math.round(angle + 90),\n        distance: rayLengthFt - 2.5,\n        width: 1,\n        x,\n        y,\n        borderColor: '#000000',\n        fillColor: game.user.color,\n        'flags.world.breathWeapon': actor.name\n    };\n    await canvas.scene.createEmbeddedDocuments('MeasuredTemplate', [template_data]);\n\tawait breath(x,y);\n});\n\nawait canvas.templates.placeables.find(i => i.data.flags?.world?.breathWeapon === actor.name)?.document?.delete();\n\nasync function breath(x,y){\n\tconst jb2a = {\n\t\tacid: 'jb2a.breath_weapons.fire.cone.green.02',\n\t\tcold: 'jb2a.breath_weapons.cold.cone.blue',\n\t\tfire: 'jb2a.breath_weapons.fire.cone.orange.02',\n\t\tlightning: 'jb2a.breath_weapons.fire.cone.blue.02',\n\t\tpoison: 'jb2a.breath_weapons.fire.cone.purple.02'\n\t}\n\tconst buttons = Object.entries(jb2a).map(i => ({label: i[0].titleCase(), value: i[0]}))\n\tconst data = {buttons, title: 'Breath Weapon'}\n\tconst type = await warpgate.buttonDialog(data, 'column')\n\n\tif(!type) return canvas.templates.placeables.find(i => i.data.flags?.world?.breathWeapon === actor.name)?.document?.delete()\n\t\n\tconst file = jb2a[type];\n\n\tnew Sequence()\n\t\t.effect()\n\t\t\t.file(file)\n\t\t\t.atLocation({x,y})\n\t\t\t.rotateTowards({x: mpx, y: mpy})\n\t\t.play()\n\t\t\n\tawait wait(1000);\n\tconst formulaGroup = type === 'acid' ? 0 : type === 'cold' ? 1 : type === 'fire' ? 2 : type === 'lightning' ? 3 : type === 'poison' ? 4 : 'wtf?';\n\tif(formulaGroup === 'wtf?') return ui.notifications.error('WTF?');\n\tawait item.rollDamage({fastForward: true, spellLevel: roll, formulaGroup});\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"custom-character-sheet-sections":{"sectionName":""}}},{"_id":"vDNVo22XmEZElli3","name":"[Storm Wyrmling] Call Lightning","type":"spell","img":"icons/magic/lightning/bolts-strike-salvo-blue.webp","data":{"description":{"value":"<p>A storm cloud appears in the shape of a cylinder that is 10 feet tall with a 60-foot radius, centered on a point you can see within range directly above you. The spell fails if you can't see a point in the air where the storm cloud could appear (for example, if you are in a room that can't accommodate the cloud).</p>\n<p>When you cast the spell, choose a point you can see under the cloud. A bolt of lightning flashes down from the cloud to that point. Each creature within 5 feet of that point must make a Dexterity saving throw. A creature takes 3d10 lightning damage on a failed save, or half as much damage on a successful one. On each of your turns until the spell ends, you can use your action to call down lightning in this way again, targeting the same point or a different one.</p>\n<p>If you are outdoors in stormy conditions when you cast this spell, the spell gives you control over the existing storm instead of creating a new one. Under such conditions, the spell's damage increases by 1d10.</p>\n<p><em><strong>At Higher Levels.</strong></em> When you cast this spell using a spell slot of 4th or higher, the damage increases by 1d10 for each slot level above 3rd.</p>","chat":"<p>A storm cloud appears in the shape of a cylinder that is 10 feet tall with a 60-foot radius, centered on a point you can see within range directly above you. The spell fails if you can't see a point in the air where the storm cloud could appear (for example, if you are in a room that can't accommodate the cloud).</p>\n<p>When you cast the spell, choose a point you can see under the cloud. A bolt of lightning flashes down from the cloud to that point. Each creature within 5 feet of that point must make a Dexterity saving throw. A creature takes 3d10 lightning damage on a failed save, or half as much damage on a successful one. On each of your turns until the spell ends, you can use your action to call down lightning in this way again, targeting the same point or a different one. <br />If you are outdoors in stormy conditions when you cast this spell, the spell gives you control over the existing storm instead of creating a new one. Under such conditions, the spell's damage increases by 1d10.</p>\n<p><strong>At Higher Levels.</strong> When you cast this spell using a spell slot of 4th or higher level, the damage increases by 1d10 for each slot level above 3rd.</p>","unidentified":""},"source":"Storm Wyrmling","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":10,"units":"minute"},"target":{"value":60,"width":null,"units":"ft","type":"cylinder"},"range":{"value":120,"long":null,"units":"ft"},"uses":{"value":0,"max":"0","per":""},"consume":{"type":"","target":"","amount":null},"ability":"str","actionType":"save","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[["3d10","lightning"],["4d10","lightning"]],"versatile":""},"formula":"","save":{"ability":"dex","dc":null,"scaling":"str"},"level":3,"school":"con","components":{"value":"","vocal":true,"somatic":true,"material":false,"ritual":false,"concentration":true},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"pact","prepared":false},"scaling":{"mode":"level","formula":"1d10"},"attunement":0,"weight":null,"price":null},"effects":[],"folder":"5mKzbDO1aQGznkqw","sort":800000,"permission":{"ssK6qqhMHUvM1A06":3,"Yusxhdg6OuCY4opM":3},"flags":{"core":{"sourceId":"Compendium.world.items-spells.iO7yxkIwGCoCoBti"},"mre-dnd5e":{"formulaGroups":[{"label":"Conjured Storm","formulaSet":[0]},{"label":"Natural Storm","formulaSet":[1]}]},"itemacro":{"macro":{"data":{"_id":null,"name":"Call Lightning","type":"script","author":"ssK6qqhMHUvM1A06","img":"icons/svg/dice-target.svg","scope":"global","command":"/* required modules:\n\t- Item Macro.\n\t- Warpgate.\n\t- Sequencer.\n\t- JB2A Patreon.\n\t- Minimal Rolling Enhancements.\n*/\n\nconst icon = item.img;\nconst file = \"jb2a.lightning_strike.01\";\n\nlet concentrating = actor.effects.find(i => i.data?.origin === item.uuid) ?? false;\n\n\nif(!concentrating){\n\tconst retVal = await warpgate.buttonDialog({buttons: [{label: \"Conjured Storm\", value: \"conjured\"}, {label: \"Natural Storm\", value: \"natural\"}]});\n\tif(!retVal) return;\n\t\n\tconst roll = await item.roll();\n\tif(!roll) return;\n\tawait lightningStrike(false, retVal);\n} else await lightningStrike(true);\n\n\n\nasync function lightningStrike(display = true, stormType = false){\n\tSequencer.Preloader.preloadForClients(file);\n\tconst {x,y,cancelled} = await warpgate.crosshairs.show({size: 3, icon});\n\tif(cancelled) return;\n\t\n\tconst templateData = {\n\t\tt: \"circle\",\n\t\tuser: game.user.id,\n\t\tdistance: 7.5,\n\t\tdirection: 0,\n\t\tx,\n\t\ty\n\t};\n\t\n\tnew Sequence()\n\t\t.effect()\n\t\t\t.file(file)\n\t\t\t.atLocation({x,y})\n\t\t\t.scale(2)\n\t\t\t.waitUntilFinished()\n\t\t.play();\n\t\n\tawait canvas.scene.createEmbeddedDocuments(\"MeasuredTemplate\", [templateData]);\n\t\n\tconcentrating = actor.effects.find(i => i.data?.origin === item.uuid) ?? false;\n\tif(stormType) await concentrating?.setFlag(\"concentrationnotifier\", \"stormType\", stormType === \"natural\" ? 1 : 0);\n\t\n\tconst spellLevel = concentrating?.getFlag(\"concentrationnotifier\", \"spellLevel\") ?? 3;\n\tconst formulaGroup = concentrating?.getFlag(\"concentrationnotifier\", \"stormType\") ?? 0;\n\tconst message = concentrating?.getFlag(\"concentrationnotifier\", \"message\") ?? false;\n\tif(display && message) ChatMessage.create(message);\n\tawait item.rollDamage({spellLevel, fastForward: true, critical: false, formulaGroup})\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"custom-character-sheet-sections":{"sectionName":""}}},{"_id":"wGx59b5yDpsQe6o7","name":"Control Water","type":"spell","img":"icons/magic/water/pseudopod-swirl-blue.webp","data":{"description":{"value":"<p>Until the spell ends, you control any freestanding water inside an area you choose that is a cube up to 100 feet on a side. You can choose from any of the following effects when you cast this spell. As an action on your turn, you can repeat the same effect or choose a different one.</p>\n<p><em><strong>Flood.</strong></em> You cause the water level of all standing water in the area to rise by as much as 20 feet. If the area includes a shore, the flooding water spills over onto dry land.</p>\n<p>If you choose an area in a large body of water, you instead create a 20-foot tall wave that travels from one side of the area to the other and then crashes down. Any Huge or smaller vehicles in the wave's path are carried with it to the other side. Any Huge or smaller vehicles struck by the wave have a 25 percent chance of capsizing.</p>\n<p>The water level remains elevated until the spell ends or you choose a different effect. If this effect produced a wave, the wave repeats on the start of your next turn while the flood effect lasts.</p>\n<p><em><strong>Part Water.</strong></em> You cause water in the area to move apart and create a trench. The trench extends across the spell's area, and the separated water forms a wall to either side. The trench remains until the spell ends or you choose a different effect. The water then slowly fills in the trench over the course of the next round until the normal water level is restored.</p>\n<p><em><strong>Redirect Flow.</strong></em> You cause flowing water in the area to move in a direction you choose, even if the water has to flow over obstacles, up walls, or in other unlikely directions. The water in the area moves as you direct it, but once it moves beyond the spell's area, it resumes its flow based on the terrain conditions. The water continues to move in the direction you chose until the spell ends or you choose a different effect.</p>\n<p><em><strong>Whirlpool.</strong></em> This effect requires a body of water at least 50 feet square and 25 feet deep. You cause a whirlpool to form in the center of the area. The whirlpool forms a vortex that is 5 feet wide at the base, up to 50 feet wide at the top, and 25 feet tall. Any creature or object in the water and within 25 feet of the vortex is pulled 10 feet toward it. A creature can swim away from the vortex by making a Strength (Athletics) check against your spell save DC.</p>\n<p>When a creature enters the vortex for the first time on a turn or starts its turn there, it must make a Strength saving throw. On a failed save, the creature takes 2d8 bludgeoning damage and is caught in the vortex until the spell ends. On a successful save, the creature takes half damage, and isn't caught in the vortex. A creature caught in the vortex can use its action to try to swim away from the vortex as described above, but has disadvantage on the Strength (Athletics) check to do so.</p>\n<p>The first time each turn that an object enters the vortex, the object takes 2d8 bludgeoning damage; this damage occurs each round it remains in the vortex.</p>","chat":"<p>Until the spell ends, you control any freestanding water inside an area you choose that is a cube up to 100 feet on a side. You can choose from any of the following effects when you cast this spell. As an action on your turn, you can repeat the same effect or choose a different one.</p>\n<p><strong>Flood.</strong> You cause the water level of all standing water in the area to rise by as much as 20 feet. If the area includes a shore, the flooding water spills over onto dry land.</p>\n<p>If you choose an area in a large body of water, you instead create a 20-foot tall wave that travels from one side of the area to the other and then crashes down. Any Huge or smaller vehicles in the wave's path are carried with it to the other side. Any Huge or smaller vehicles struck by the wave have a 25 percent chance of capsizing.</p>\n<p>The water level remains elevated until the spell ends or you choose a different effect. If this effect produced a wave, the wave repeats on the start of your next turn while the flood effect lasts.</p>\n<p><strong>Part Water.</strong> You cause water in the area to move apart and create a trench. The trench extends across the spell's area, and the separated water forms a wall to either side. The trench remains until the spell ends or you choose a different effect. The water then slowly fills in the trench over the course of the next round until the normal water level is restored.</p>\n<p><strong>Redirect Flow.</strong> You cause flowing water in the area to move in a direction you choose, even if the water has to flow over obstacles, up walls, or in other unlikely directions. The water in the area moves as you direct it, but once it moves beyond the spell's area, it resumes its flow based on the terrain conditions. The water continues to move in the direction you chose until the spell ends or you choose a different effect.</p>\n<p><strong>Whirlpool.</strong> This effect requires a body of water at least 50 feet square and 25 feet deep. You cause a whirlpool to form in the center of the area. The whirlpool forms a vortex that is 5 feet wide at the base, up to 50 feet wide at the top, and 25 feet tall. Any creature or object in the water and within 25 feet of the vortex is pulled 10 feet toward it. A creature can swim away from the vortex by making a Strength (Athletics) check against your spell save DC.</p>\n<p>When a creature enters the vortex for the first time on a turn or starts its turn there, it must make a Strength saving throw. On a failed save, the creature takes 2d8 bludgeoning damage and is caught in the vortex until the spell ends. On a successful save, the creature takes half damage, and isn't caught in the vortex. A creature caught in the vortex can use its action to try to swim away from the vortex as described above, but has disadvantage on the Strength (Athletics) check to do so.</p>\n<p>The first time each turn that an object enters the vortex, the object takes 2d8 bludgeoning damage; this damage occurs each round it remains in the vortex.</p>","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":10,"units":"minute"},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":300,"long":null,"units":"ft"},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"save","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[["2d8[bludgeoning - Whirlpool]","bludgeoning"]],"versatile":""},"formula":"","save":{"ability":"str","dc":null,"scaling":"spell"},"level":4,"school":"trs","components":{"value":"a drop of water and a pinch of dust","vocal":true,"somatic":true,"material":true,"ritual":false,"concentration":true},"materials":{"value":"a drop of water and a pinch of dust","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"pact","prepared":false},"scaling":{"mode":"none","formula":""},"attunement":null},"effects":[],"folder":null,"sort":1000000,"permission":{"default":0,"Yusxhdg6OuCY4opM":3},"flags":{"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[0]}]},"core":{"sourceId":"Item.eIKhfMeTudkG4IMM"}}},{"_id":"OxKFPBvlWdjlcIK3","name":"Counterspell","type":"spell","img":"icons/magic/defensive/shield-barrier-blades-teal.webp","data":{"description":{"value":"<p>You attempt to interrupt a creature in the process of casting a spell. If the creature is casting a spell of 3rd level or lower, its spell fails and has no effect. If it is casting a spell of 4th level or higher, make an ability check using your spellcasting ability. The DC equals 10 + the spell's level. On a success, the creature's spell fails and has no effect.</p>\n<p><em><strong>At Higher Levels.</strong></em> When you cast this spell using a spell slot of 4th level or higher, the interrupted spell has no effect if its level is less than or equal to the level of the spell slot you used.</p>","chat":"<p>You attempt to interrupt a creature in the process of casting a spell. If the creature is casting a spell of 3rd level or lower, its spell fails and has no effect. If it is casting a spell of 4th level or higher, make an ability check using your spellcasting ability. The DC equals 10 + the spell's level. On a success, the creature's spell fails and has no effect.</p>\n<p><strong>At Higher Levels.</strong> When you cast this spell using a spell slot of 4th level or higher, the interrupted spell has no effect if its level is less than or equal to the level of the spell slot you used.</p>","unidentified":""},"source":"","activation":{"type":"reaction","cost":1,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":1,"width":null,"units":"","type":"creature"},"range":{"value":60,"long":null,"units":"ft"},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":"cha","actionType":"abil","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":3,"school":"abj","components":{"value":"","vocal":false,"somatic":true,"material":false,"ritual":false,"concentration":false},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"pact","prepared":false},"scaling":{"mode":"none","formula":""},"attunement":null},"effects":[],"folder":null,"sort":1100000,"permission":{"default":0,"Yusxhdg6OuCY4opM":3},"flags":{"favtab":{"isFavorite":false},"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[]}]},"core":{"sourceId":"Item.tXgZPRtCS2TJ6DJE"}}},{"_id":"TaqlhVd2Zc5YVYaE","name":"Create or Destroy Water","type":"spell","img":"icons/magic/water/orb-water-bubbles.webp","data":{"description":{"value":"<p>You either create or destroy water.</p>\n<p><em><strong>Create Water.</strong></em> You create up to 10 gallons of clean water within range in an open container. Alternatively, the water falls as rain in a 30-foot cube within range, extinguishing exposed flames in the area.</p>\n<p><em><strong>Destroy Water.</strong></em> You destroy up to 10 gallons of water in an open container within range. Alternatively, you destroy fog in a 30-foot cube within range.</p>\n<p><em><strong>At Higher Levels.</strong></em> When you cast this spell using a spell slot of 2nd level or higher, you create or destroy 10 additional gallons of water, or the size of the cube increases by 5 feet, for each slot level above 1st.</p>","chat":"<p>You either create or destroy water.</p>\n<p><strong>Create Water.</strong> You create up to 10 gallons of clean water within range in an open container. Alternatively, the water falls as rain in a 30-foot cube within range, extinguishing exposed flames in the area.</p>\n<p><strong>Destroy Water.</strong> You destroy up to 10 gallons of water in an open container within range. Alternatively, you destroy fog in a 30-foot cube within range.</p>\n<p><strong>At Higher Levels.</strong> When you cast this spell using a spell slot of 2nd level or higher, you create or destroy 10 additional gallons of water, or the size of the cube increases by 5 feet, for each slot level above 1st.</p>","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":30,"long":null,"units":"ft"},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":1,"school":"trs","components":{"value":"a drop of water if creating water or a few grains of sand if destroying it","vocal":true,"somatic":true,"material":true,"ritual":false,"concentration":false},"materials":{"value":"a drop of water if creating water or a few grains of sand if destroying it","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"pact","prepared":false},"scaling":{"mode":"none","formula":""},"attunement":null},"effects":[],"folder":null,"sort":1200000,"permission":{"default":0,"Yusxhdg6OuCY4opM":3},"flags":{"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[]}]},"itemacro":{"macro":{"data":{"_id":null,"name":"Create or Destroy Water","type":"script","author":"ssK6qqhMHUvM1A06","img":"icons/svg/dice-target.svg","scope":"global","command":"/* CREATE OR DESTROY WATER\n * option 1: water in a container\n * option 2: 30-foot cube (+5 feet per spell level above 1st)\n */\n\n// get level of spell being cast\nconst r = await warpgate.dnd5e.rollItem(item);\nconsole.log(`The spell, ${item.name}, was cast at level ${r}.`);\nif(r < 1) return;\n\n// size of cube:\nconst cube = 5*r + 25;\nconsole.log(`The cube is going to be ${cube}-by-${cube} feet.`);\n\n// the template:\nlet template_data = {\n\tt: 'ray',\n\tuser: game.userId,\n\tdirection: 0,\n\tangle: 0,\n\tdistance: cube,\n\twidth: cube,\n\tborderColor: '#000000',\n\tfillColor: '#3898B9',\n\tx: canvas.app.renderer.plugins.interaction.mouse.getLocalPosition(canvas.app.stage).x,\n\ty: canvas.app.renderer.plugins.interaction.mouse.getLocalPosition(canvas.app.stage).y\n};\n\n// prompt for option 1 or 2:\nconst retVal = await warpgate.buttonDialog({\n\tbuttons : [\n\t\t{label : `Create/destroy water in container`, value : 'opt1'},\n\t\t{label : `Conjure rain in ${cube}-foot cube`, value : 'opt2create'},\n\t\t{label : `Destroy fog in ${cube}-foot cube`, value : 'opt2destroy'},\n\t],\n\ttitle : item.name\n}, 'column');\n\nif(retVal === 'opt2create' || retVal === 'opt2destroy'){\n\t// do thing with x-foot cube (actually a x-foot wide and long line):\n\tlet doc = new CONFIG.MeasuredTemplate.documentClass(template_data, {parent: canvas.scene});\n\tlet template = new game.dnd5e.canvas.AbilityTemplate(doc);\n\ttemplate.actorSheet = actor.sheet;\n\ttemplate.drawPreview();\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"core":{"sourceId":"Item.NsX8aRVp9fIQ6o7V"}}},{"_id":"lhUvcGVbDytQK5us","name":"Geyser","type":"spell","img":"icons/magic/sonic/explosion-shock-wave-teal.webp","data":{"description":{"value":"<p>You cause a massive eruption of water to blast upwards from the ground at a point within range. Creatures within 10 feet of the point must make a Dexterity save or take 4d6 bludgeoning damage and be knocked 60 feet into the air. On a successful save, creatures take half as much damage, and are instead knocked their choice of 10 feet away from the point or 10 feet upward.</p>","chat":"","unidentified":""},"source":"Kibbles' Generic Spells","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":10,"width":null,"units":"ft","type":"radius"},"range":{"value":120,"long":null,"units":"ft"},"uses":{"value":0,"max":"0","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"save","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[["4d6","bludgeoning"],["6d6","bludgeoning"]],"versatile":""},"formula":"","save":{"ability":"dex","dc":null,"scaling":"spell"},"level":4,"school":"con","components":{"value":"","vocal":true,"somatic":true,"material":false,"ritual":false,"concentration":false},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"pact","prepared":false},"scaling":{"mode":"none","formula":""},"attunement":0},"effects":[],"folder":null,"sort":1300000,"permission":{"Yusxhdg6OuCY4opM":3},"flags":{"cf":{"id":"temp_o4tqzvf5ecs","path":"Spells#/CF_SEP/Conjuration#/CF_SEP/Conjuration-4th","color":"#000000"},"core":{"sourceId":"Compendium.world.items-spells.bhfT4RiGLZkrOCpR"},"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[0]},{"label":"Fall damage","formulaSet":[1]}]}}},{"_id":"rSpihIZ9Kg5In0S8","name":"Invisibility","type":"spell","img":"icons/magic/perception/silhouette-stealth-shadow.webp","data":{"description":{"value":"<p>A creature you touch becomes invisible until the spell ends. Anything the target is wearing or carrying is invisible as long as it is on the target's person. The spell ends for a target that attacks or casts a spell.</p>\n<p><em><strong>At Higher Levels.</strong></em> When you cast this spell using a spell slot of 3rd level or higher, you can target one additional creature for each slot level above 2nd.</p>","chat":"<p>A creature you touch becomes invisible until the spell ends. Anything the target is wearing or carrying is invisible as long as it is on the target's person. The spell ends for a target that attacks or casts a spell.</p>\n<p><strong>At Higher Levels.</strong> When you cast this spell using a spell slot of 3rd level or higher, you can target one additional creature for each slot level above 2nd.</p>","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":1,"units":"hour"},"target":{"value":1,"width":null,"units":"","type":"creature"},"range":{"value":null,"long":null,"units":"touch"},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"other","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":2,"school":"ill","components":{"value":"an eyelash encased in gum arabic","vocal":true,"somatic":true,"material":true,"ritual":false,"concentration":true},"materials":{"value":"an eyelash encased in gum arabic","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"pact","prepared":false},"scaling":{"mode":"none","formula":""},"attunement":null},"effects":[],"folder":null,"sort":1550000,"permission":{"default":0,"Yusxhdg6OuCY4opM":3},"flags":{"dae":{"activeEquipped":false,"alwaysActive":false},"itemacro":{"macro":{"data":{"_id":null,"name":"Invisibility","type":"script","author":"ssK6qqhMHUvM1A06","img":"icons/svg/dice-target.svg","scope":"global","command":"","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"favtab":{"isFavorite":false},"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[]}]},"core":{"sourceId":"Item.CtWPCdD3pJR1yIec"}}},{"_id":"RLFC6IRbxPqAOGvC","name":"Jet of Water","type":"spell","img":"icons/magic/light/beam-rays-blue-large.webp","data":{"description":{"value":"<p>You open a channel to the Elemental Plane of Water for a brief time. A jet of saltwater forming a line 60 feet long and 5 feet wide blasts out from you in a direction you choose for the spell’s duration. Each creature that starts its turn in the line or enters it for the first time on a turn must make a Strength saving throw. On a failed save, a creature takes 4d6 bludgeoning damage and is knocked prone. On a successful save, a creature takes half as much damage and isn’t knocked prone.</p>\n<p>In addition, any creature in the line must spend 2 feet of movement for every 1 foot it moves when moving closer to you.</p>\n<p>The jet extinguishes flames in the area. Medium or smaller objects in the line that aren’t secured to anything and aren’t worn or carried by anyone are knocked over or pushed up to 15 feet away from you.</p>\n<p>As a bonus action on each of your turns before the spell ends, you can change the direction in which the line blasts from you.</p>\n<p><em><strong>At Higher Levels.</strong></em> When you cast this spell using a spell slot of 4th level or higher, the damage increases by 1d6 for each slot level above 3rd.</p>","chat":"<p>You open a channel to the Elemental Plane of Water for a brief time. A jet of saltwater forming a line 60 feet long and 5 feet wide blasts out from you in a direction you choose for the spell’s duration. Each creature that starts its turn in the line or enters it for the first time on a turn must make a Strength saving throw. On a failed save, a creature takes 4d6 bludgeoning damage and is knocked prone. On a successful save, a creature takes half as much damage and isn’t knocked prone.</p>\n<p>In addition, any creature in the line must spend 2 feet of movement for every 1 foot it moves when moving closer to you.</p>\n<p>The jet extinguishes flames in the area. Medium or smaller objects in the line that aren’t secured to anything and aren’t worn or carried by anyone are knocked over or pushed up to 15 feet away from you.</p>\n<p>As a bonus action on each of your turns before the spell ends, you can change the direction in which the line blasts from you.</p>\n<p><em><strong>At Higher Levels.</strong></em> When you cast this spell using a spell slot of 4th level or higher, the damage increases by 1d6 for each slot level above 3rd.</p>","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":1,"units":"minute"},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":"self"},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"save","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[["4d6[bludgeoning]","bludgeoning"]],"versatile":""},"formula":"","save":{"ability":"str","dc":null,"scaling":"spell"},"level":3,"school":"con","components":{"value":"a small hollow tube","vocal":true,"somatic":true,"material":true,"ritual":false,"concentration":true},"materials":{"value":"a small hollow tube","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"pact","prepared":false},"scaling":{"mode":"level","formula":"1d6"},"attunement":0},"effects":[],"folder":null,"sort":1600000,"permission":{"default":0,"ssK6qqhMHUvM1A06":3,"Yusxhdg6OuCY4opM":3},"flags":{"core":{"sourceId":"Compendium.world.ddb-havilon-spells.WXZp4ouio8k8JsdG"},"cf":{"id":"temp_dcwwk81in1l","path":"Conjuration#/CF_SEP/Conjuration-3rd","color":"#000000"},"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[0]}]},"itemacro":{"macro":{"data":{"_id":null,"name":"Jet of Water","type":"script","author":"ssK6qqhMHUvM1A06","img":"icons/svg/dice-target.svg","scope":"global","command":"// JET OF WATER\n// Required modules:\n// - Item Macro\n// - Z's Concentration Notifier\n\n\n// concentrating on this, yay or nay?\nconst effect = actor.effects.find(i => i.data?.origin === item.uuid) ?? false;\n\nif(!effect){\n\tconst roll = await item.roll();\n\tif(!roll) return;\n} else {\n\tconst spellLevel = effect.getFlag(\"concentrationnotifier\", \"spellLevel\");\n\tconst message = effect.getFlag(\"concentrationnotifier\", \"message\");\n\tChatMessage.create(message);\n\t//await item.rollDamage({spellLevel, fastForward: true, critical: false, formulaGroup})\n}\n\nasync function wait(ms) {\n    return new Promise(resolve => {\n        setTimeout(resolve, ms);\n    });\n}\nfunction toDegrees(rads) {\n    return (rads * 180 / Math.PI) + 90;\n}\n\nconst rayLengthFt = 60;\nconst rayLengthPx = canvas.grid.size * rayLengthFt / canvas.scene.data.gridDistance;\nconst startPoint = {x: token.center.x, y: token.center.y};\nlet angle = 0;\nlet start = 0;\nlet request;\nconst fill = game.user.data.color;\n\nasync function onLoad(){\n    let rayLine = new PIXI.Graphics();\n    rayLine.position.set(startPoint.x, startPoint.y);\n    rayLine.lineStyle(15, fill.replace(\"#\",\"0x\"), 0.5)\n       .moveTo(0, 0)\n       .lineTo(0, rayLengthPx);\n    rayLine.name = \"tempRay\";\n    canvas.app.stage.addChild(rayLine);\n    requestAnimationFrame(step);\n}\nfunction step(timestamp) {\n    let marker = canvas.app.stage.children.find(c => c.name == \"tempRay\");\n    if (start === 0) start = timestamp;\n    const elapsed = timestamp - start;\n    const mousePos = canvas.app.renderer.plugins.interaction.mouse.getLocalPosition(canvas.app.stage);\n    angle = mousePos.x === startPoint.x ? toDegrees(Math.PI / 2) : \n\t\tmousePos.x > startPoint.x ? toDegrees(Math.PI - Math.atan((startPoint.y - mousePos.y)/(mousePos.x - startPoint.x))) :\n\t\tmousePos.x < startPoint.x ? toDegrees(Math.atan((mousePos.y - startPoint.y)/(mousePos.x - startPoint.x))) : 0; //Math.ceil(elapsed * 360 / 1000);\n    marker.angle = angle\n    if (elapsed < 1000) request = requestAnimationFrame(step);\n    else { \n        start = 0;\n        console.log(angle)\n        request = requestAnimationFrame(step);\n    }\n}\n\nfunction offSet(angle){\n\tconst halfGrid = canvas.grid.size / 2;\n\tconst angleR = Math.round(angle/45)*45;\n\tconsole.log(angleR);\n\tlet x, y;\n\t\n\tif([0, 180, 360].includes(angleR)) x = 0;\n\telse if([45, 90, 135].includes(angleR)) x = -halfGrid;\n\telse x = halfGrid;\n\t\n\tif([0, 45, 315, 360].includes(angleR)) y = halfGrid;\n\telse if([135, 180, 225].includes(angleR)) y = -halfGrid;\n\telse y = 0;\n\t\n\tconsole.log(x,y);\n\t\n\treturn [x,y];\n}\n\n\n\nawait PIXI.Loader.shared\n    .load(onLoad);\n\nawait wait(500);\n\n$(document.body).on(\"click\", async () => {\n    $(document.body).off(\"click\");\n    cancelAnimationFrame(request);\n    canvas.app.stage.removeChild(canvas.app.stage.children.find(c => c.name === \"tempRay\"));\n\t\n\tlet template_data = {\n\t\tt: 'ray',\n\t\tuser: game.userId,\n\t\tdirection: angle + 90,\n\t\tdistance: rayLengthFt,\n\t\twidth: 5,\n\t\tx: startPoint.x + offSet(angle)[0],\n\t\ty: startPoint.y + offSet(angle)[1],\n\t\tborderColor: '#000000',\n\t\tfillColor: '#3898B9',\n\t\t'flags.world.jet-of-water': actor.name\n\t};\n\tlet ranges = await canvas.scene.createEmbeddedDocuments('MeasuredTemplate', [template_data]);\n});\n\nconst oldTemplate = canvas.templates.getDocuments().find(i => i.getFlag(\"world\", \"jet-of-water\") === actor.name);\nif(oldTemplate) await oldTemplate.delete();","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}}}},{"_id":"3AD77bPJijGpCHGj","name":"Thunder Step","type":"spell","img":"icons/magic/lightning/bolt-strike-blue-white.webp","data":{"description":{"value":"<p>You teleport yourself to an unoccupied space you can see within range. Immediately after you disappear, a thunderous boom sounds, and each creature within 10 feet of the space you left must make a Constitution saving throw, taking 3d10 thunder damage on a failed save, or half as much damage on a successful one. The thunder can be heard from up to 300 feet away.</p>\n<p>You can bring along objects as long as their weight doesn't exceed what you can carry. You can also teleport one willing creature of your size or smaller who is carrying gear up to its carrying capacity. The creature must be within 5 feet of you when you cast this spell, and there must be an unoccupied space within 5 feet of your destination space for the creature to appear in; otherwise, the creature is left behind.</p>\n<p><em><strong>At Higher Levels.</strong></em> When you cast this spell using a spell slot of 4th level or higher, the damage increases by 1d10 for each slot level above 3rd.</p>","chat":"<p class=\"Core-Styles_Core-Body\">You teleport yourself to an unoccupied space you can see within range. Immediately after you disappear, a thunderous boom sounds, and each creature within 10 feet of the space you left must make a Constitution saving throw, taking 3d10 thunder damage on a failed save, or half as much damage on a successful one. The thunder can be heard from up to 300 feet away.</p>\n<p class=\"Core-Styles_Core-Body\">You can bring along objects as long as their weight doesn’t exceed what you can carry. You can also teleport one willing creature of your size or smaller who is carrying gear up to its carrying capacity. The creature must be within 5 feet of you when you cast this spell, and there must be an unoccupied space within 5 feet of your destination space for the creature to appear in; otherwise, the creature is left behind.</p>\n<p class=\"Core-Styles_Core-Body\"><span class=\"Serif-Character-Style_Inline-Subhead-Serif\"><strong>At Higher Levels.</strong> </span>When you cast this spell using a spell slot of 4th level or higher, the damage increases by 1d10 for each slot level above 3rd.</p>","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":90,"long":null,"units":"ft"},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"save","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[["3d10","thunder"]],"versatile":""},"formula":"","save":{"ability":"con","dc":null,"scaling":"spell"},"level":3,"school":"con","components":{"value":"","vocal":true,"somatic":false,"material":false,"ritual":false,"concentration":false},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"pact","prepared":false},"scaling":{"mode":"level","formula":"1d10"},"attunement":null},"effects":[],"folder":null,"sort":1700000,"permission":{"default":0,"Yusxhdg6OuCY4opM":3},"flags":{"itemacro":{"macro":{"data":{"_id":null,"name":"Thunder Step","type":"script","author":"ssK6qqhMHUvM1A06","img":"icons/svg/dice-target.svg","scope":"global","command":"/* Requires Warp Gate, Item Macro, Sequencer, JB2A */\n\ntoken.actor.sheet.minimize();\n\nlet cachedDistance = 0;\nconst spellName = \"Thunder Step\";\nconst maxDist = 90; // range of the spell\nconst file = \"jb2a.thunderwave.center.blue\";\n\n\nconst checkDistance = async(crosshairs) => {\n\twhile (crosshairs.inFlight) {\n\t\t//wait for initial render\n\t\tawait warpgate.wait(100);\n\t\tconst ray = new Ray( token.center, crosshairs );\n\t\tconst distance = canvas.grid.measureDistances([{ray}], {gridSpaces:true})[0];\n\t\t\n\t\t//only update if the distance has changed\n\t\tif (cachedDistance !== distance) {\n\t\t\tcachedDistance = distance;\n\t\t\tif(distance > maxDist) crosshairs.icon = 'icons/svg/hazard.svg';\n\t\t\telse crosshairs.icon = token.data.img;\n\t\t\t\n\t\t\tcrosshairs.draw();\n\t\t\tcrosshairs.label = `${distance} ft`;\n\t\t}\n\t}\n}\n\nawait Sequencer.Preloader.preloadForClients(file);\n\nconst {x,y,cancelled} = await warpgate.crosshairs.show({size: token.data.width, icon: token.data.img, label: '0 ft.'}, {show: checkDistance});\nif(cancelled) return;\nif(cachedDistance > maxDist) return ui.notifications.error(`${spellName} has a maximum range of ${maxDist} feet.`);\n\nconst roll = await item.roll();\nif(!roll) return;\n\nconst templateData = {\n\tt: \"circle\",\n\tuser: game.user.id,\n\tx: token.x + canvas.grid.size / 2,\n\ty: token.y + canvas.grid.size / 2,\n\tdirection: 0,\n\tdistance: 10,\n\tborderColor: \"#000000\",\n\tfillColor: \"#660066\"\n};\n\nawait new Sequence()\n\t.effect()\n\t\t.file(file)\n\t\t.atLocation(token)\n\t\t.size(5, {gridUnits: true})\n\t.animation()\n\t\t.on(token)\n\t\t.fadeOut(300)\n\t\t.waitUntilFinished()\n\t.thenDo( async () => {await canvas.scene.createEmbeddedDocuments(\"MeasuredTemplate\", [templateData]);})\n\t.animation()\n\t\t.on(token)\n\t\t.teleportTo({x,y}, {relativeToCenter: true})\n\t\t.fadeIn(1000)\n\t.play();\n\ntoken.actor.sheet.maximize();","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[0]}]},"core":{"sourceId":"Item.b9Ylxpm8k5D0LiSc"}}},{"_id":"7DOg1vV7ZbIpXnLi","name":"Spirit Shroud","type":"spell","img":"icons/magic/death/skull-flames-white-blue.webp","data":{"description":{"value":"<p>You call forth spirits of the dead, which flit around you for the spell’s duration. The spirits are intangible and invulnerable.</p>\n<p>Until the spell ends, any attack you make deals 1d8 extra damage when you hit a creature within 10 feet of you. This damage is radiant, necrotic, or cold (your choice when you cast the spell). Any creature that takes this damage can’t regain hit points until the start of your next turn.</p>\n<p>In addition, any creature of your choice that you can see that starts its turn within 10 feet of you has its speed reduced by 10 feet until the start of your next turn.</p>\n<p><em><strong>At Higher Levels.</strong></em> When you cast this spell using a spell slot of 4th level or higher, the damage increases by 1d8 for every two slot levels above 3rd.</p>","chat":"<p>You call forth spirits of the dead, which flit around you for the spell’s duration. The spirits are intangible and invulnerable.</p>\n<p>Until the spell ends, any attack you make deals 1d8 extra damage when you hit a creature within 10 feet of you. This damage is radiant, necrotic, or cold (your choice when you cast the spell). Any creature that takes this damage can’t regain hit points until the start of your next turn.</p>\n<p>In addition, any creature of your choice that you can see that starts its turn within 10 feet of you has its speed reduced by 10 feet until the start of your next turn.</p>\n<p><em><strong>At Higher Levels.</strong></em> When you cast this spell using a spell slot of 4th level or higher, the damage increases by 1d8 for every two slot levels above 3rd.</p>","unidentified":""},"source":"","activation":{"type":"bonus","cost":1,"condition":""},"duration":{"value":1,"units":"minute"},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":"self"},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"","attackBonus":"0","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":3,"school":"nec","components":{"value":"","vocal":true,"somatic":true,"material":false,"ritual":false,"concentration":true},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"pact","prepared":false},"scaling":{"mode":"none","formula":""},"attunement":0},"effects":[],"folder":null,"sort":1800000,"permission":{"default":0,"Yusxhdg6OuCY4opM":3,"ssK6qqhMHUvM1A06":3},"flags":{"itemacro":{"macro":{"data":{"_id":null,"name":"Spirit Shroud","type":"script","author":"ssK6qqhMHUvM1A06","img":"icons/svg/dice-target.svg","scope":"global","command":"const name = \"Spirit Shroud\";\nconst effect = actor.effects.find(i => i.data?.origin === item.uuid) ?? false; // concentrating on Spirit Shroud, yes no?\n\n/* if effect, delete it */\nif(effect) return effect.delete();\n\n/* else cast spell */\nconst roll = await warpgate.dnd5e.rollItem(item);\nif(!roll) return;\n\nconst damageType = await warpgate.buttonDialog({buttons: [{label: 'Cold', value: 'cold'}, {label: 'Necrotic', value: 'necrotic'}], title: name}, 'column') ?? 'cold';\nconst mode = CONST.ACTIVE_EFFECT_MODES.ADD;\nconst value = `+${Math.ceil(roll/2) - 1}d8[${damageType}]`;\nconst priority = 20;\n\nconst changes = [{key: \"data.bonuses.mwak.damage\", mode, value, priority}, {key: \"data.bonuses.msak.damage\", mode, value, priority}];\nconst concentration = actor.effects.find(i => i.data?.origin === item.uuid) ?? false;\nif(!concentration) return ui.notifications.error(\"Error: effect not found.\");\nawait concentration.update({changes});","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"core":{"sourceId":"Item.Ac3jJxkiQwZgDau4"},"mre-dnd5e":{"formulaGroups":[{"label":"Primary","formulaSet":[]}]},"favtab":{"isFavorite":false}}},{"_id":"DJAdQvVzUQVUHd4Q","name":"Unarmed Strike","type":"feat","img":"icons/skills/melee/unarmed-punch-fist.webp","data":{"description":{"value":"<p>A punch, kick, head-butt, or similar forceful blow (none of which count as weapons). On a hit, an unarmed strike deals bludgeoning damage equal to 1 + your Strength modifier. You are proficient with your unarmed strikes.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":1,"width":null,"units":"","type":"creature"},"range":{"value":5,"long":null,"units":"ft"},"uses":{"value":null,"max":"","per":""},"consume":{"type":"","target":"","amount":null},"ability":"str","actionType":"mwak","attackBonus":"0","chatFlavor":"","critical":{"thresh