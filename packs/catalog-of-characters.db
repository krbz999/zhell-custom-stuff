value : 'Cancel'}\n\t\t],\n\t\toptions : { title : 'Stow in the bag', defaultButton : 'Cancel' }\n\t});\n\n\tif(putInBagMenu.buttons === 'Cancel') return;\n\n\tconst toDelete = [];\n\tconst toCreate = [];\n\tlet theItem;\n\n\tfor(let j = 0; j < optionsPutInBag.length; j++){\n\t\tif(putInBagMenu.inputs[j]){\n\t\t\ttoDelete.push(inp[j].id);\n\t\t\ttheItem = actor.items.get(inp[j].id);\n\t\t\ttoCreate.push(theItem.toObject());\n\t\t}\n\t}\n\t\n\tconst created = await container.createEmbeddedDocuments('Item',toCreate);\n\tawait actor.deleteEmbeddedDocuments('Item',toDelete);\n\t\n\t// move weapons and equipment to cargo instead of having them as vehicle weapon/upgrades.\n\tlet updates = [];\n\tcontainer.items.contents.forEach(t => { updates.push({_id : t.id, 'data.equipped' : false, 'data.attunement' : Math.min(t.data.data.attunement, 1)}) });\n\tawait container.updateEmbeddedDocuments('Item', updates);\n\t\n\tfor(let x of created){\n\t\tawait x.setFlag('dnd5e', 'vehicleCargo', true);\n\t\t//await x.update({ 'data.equipped' : false, 'data.attunement' : Math.min(x.data.data.attunement, 1)}); // DONT UPDATE INSIDE A LOOP\n\t}\n}\n\n/** Show bag sheet. Do nothing else. **/\nasync function showBag(){\n\tawait container.sheet.render(true);\n}\n\nlet mainMenu = await warpgate.menu({\n\tbuttons : [\n\t\t{ label : 'Retrieve from the bag', value : 'take' },\n\t\t{ label : 'Stow in the bag', value : 'stow' },\n\t\t{ label : 'Show the contents', value : 'show' },\n\t\t{ label : 'Cancel', value : 'cancel' }\n\t],\n\toptions : { title : item.name, defaultButton : 'cancel' }\n});\n\nif(mainMenu.buttons === 'cancel') return;\nif(mainMenu.buttons === 'take') takeFromBag();\nif(mainMenu.buttons === 'stow') putInBag();\nif(mainMenu.buttons === 'show') showBag();","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}}}},{"_id":"MNNSQ96bf6pWA0Wa","name":"Vial","type":"loot","img":"icons/consumables/potions/bottle-round-empty-glass.webp","data":{"description":{"value":"<p>A vial ca